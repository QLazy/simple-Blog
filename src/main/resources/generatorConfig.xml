<!--     	<columnOverride column="commentCount" javaType="java.long.Integer"></columnOverride> -->
<!--     	<columnOverride column="likeCount" javaType="java.long.Integer"></columnOverride> -->
<!--     </table> -->

  </context>
</generatorConfiguration>